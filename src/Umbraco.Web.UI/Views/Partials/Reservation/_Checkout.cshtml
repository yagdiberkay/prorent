                                var economyItem = {
                                    productName: products[i].productName,
                                    productNo: products[i].productNo,
                                    totalPrice: products[i].totalPrice
                                }
                                economy.push(economyItem);
                            }


                   $.ajax({
                    type: "POST",
                       url: "@Url.Action("InsertReservation", "Reservation")",
                       data: { capacity: JSON.stringify(capacity), address: JSON.stringify(address), products: JSON.stringify(products) },
                       cache: false,
                       success: function (orderNumber) {

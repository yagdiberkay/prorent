                        function GetCountries() {
                            $.ajax({
                            type: "POST",
                            url: "@Url.Action("GetCountries", "Reservation")",
                            dataType: "json",
                            cache: false,
                                 success: function (retval) {
                                for (var i = 0; i < retval.length; i++) {
                                    var o = new Option(retval[i].countryName, retval[i].countryCode);
                                    /// jquerify the DOM object 'o' so we can use the html method
                                    $(o).html(retval[i].countryName);
                                    $("#billing_country").append(o);
                                    $("#billing_country").val("TR");
                                     }
                                     GetCities("TR");
                            },
                            error: function () {
                            }
                });
                        }
                        function GetCities(countryCode) {
                           $.ajax({
                            type: "POST",
                            url: "@Url.Action("GetCities", "Reservation")",
                            dataType: "json",
                            data: { "countryCode": countryCode},
                            cache: false,
                                 success: function (retval) {
                                for (var i = 0; i < retval.length; i++) {
                                    var o = new Option(retval[i].cityName, retval[i].cityNo);
                                    /// jquerify the DOM object 'o' so we can use the html method
                                    $(o).html(retval[i].cityName);
                                    $("#billing_state").append(o);
                                    $("#billing_state").val("598");//izmir
                                }
                            },
                            error: function () {
                            }
                });
                        }

                        function redirectFunction() {

                            var error = false;
                            $('input[name="billing_first_name"]').removeClass('stm_error');
                            $('input[name="billing_last_name"]').removeClass('stm_error');
                            $('input[name="billing_driver_license"]').removeClass('stm_error');
                            $('input[name="billing_address_1"]').removeClass('stm_error');
                            $('input[name="billing_phone"]').removeClass('stm_error');
                            $('input[name="billing_email"]').removeClass('stm_error');


                            if ($('input[name="billing_first_name"]').val() == '') {

                                $('input[name="billing_first_name"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="billing_last_name"]').val() == '') {

                                $('input[name="billing_last_name"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="billing_driver_license"]').val() == '') {

                                $('input[name="billing_driver_license"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="billing_address_1"]').val() == '') {

                                $('input[name="billing_address_1"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="billing_phone"]').val() == '') {

                                $('input[name="billing_phone"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="billing_email"]').val() == '') {

                                $('input[name="billing_email"]').addClass('stm_error');
                                error = true;
                            }
                            if ($('input[name="terms"]').prop('checked') == false) {

                                error = true;
                            }

                            if (error) {
                                e.preventDefault();
                                return;
                            }

                            var address = {
                                addressNo: 0,
                                addressCorpNo: 0,
                                addressType: 1,
                                billingAddress: true,
                                shippinAddress: true,
                                name: $("#billing_first_name").val(),
                                lastname: $("#billing_last_name").val(),
                                corpName: $("#billing_driver_license").val(),
                                corpStyle: $("#billing_company").val(),
                                address: $("#billing_address_1").val(),
                                districtName: $("#billing_city").val(),
                                cityNo: $("#billing_state").val(),
                                countryCode: $("#billing_country").val(),
                                phone11: $("#billing_phone").val(),
                                phone22: $("#billing_phone").val(),
                                mobPhone1: $("#billing_phone").val(),
                                mobPhone2: $("#billing_phone").val(),
                                phoneHome: $("#billing_phone").val(),
                                email: $("#billing_email").val()
                            };
